{
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": ["vim"],
    "disable": ["missing-fields"]
  },
  "workspace": {
    "checkThirdParty": false,
    "library": [
      "$VIMRUNTIME",
      "${3rd}/luv/library"
    ]
  },
  "telemetry": {
    "enable": false
  },
  "format": {
    "enable": false
  }
}
